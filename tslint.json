{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-sonarts"
    ],
    "jsRules": {},
    "rules": {
        "member-ordering": {
            "options": {
                "alphabetize": true,
                "order": "fields-first"
            }
        },
        "no-console": {
            "options": ["console.log"],
            "severity": "warning"
        }
    },
    "rulesDirectory": []
}